{
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1",
    "^@utils/(.*)$": ["<rootDir>/src/utils/$1"],
    "^@middlewares/(.*)$": ["<rootDir>/src/routes/middlewares/$1"],
    "^@routers/(.*)$": ["<rootDir>/src/routes/routers/$1"]
  },
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testPathIgnorePatterns": ["dist/"]
}
